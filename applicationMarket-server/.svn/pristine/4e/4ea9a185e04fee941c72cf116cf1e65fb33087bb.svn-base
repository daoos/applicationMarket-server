package com.techwells.applicationMarket.domain.rs;

import java.util.Date;

import com.techwells.applicationMarket.util.DateUtil;

/**
 * APP和version的值对象
 * @author Administrator
 *
 */
public class AppAndVersionVos {
	private Integer appId;  //应用Id
	private Integer versionId;  //版本Id
	private String appName; 
	private String size;  //大小
	private String logo;  //应用logo
	private String introduction;   //应用简介
	private String downloadUrl;  //安装包地址
	private String versionNum;  //版本号、
	private String supportSys;  //支持的系统
