	</if>
	
	<if test="params.authDate!=null">
		and a.create_date=#{params.authDate}
	</if>
	
	<if test="params.status!=null">
		and a.examine__status=#{params.status}
	</if>
</where>
order by a.create_date desc
limit #{startNum},#{pageSize}
 </select>
 
 
 
